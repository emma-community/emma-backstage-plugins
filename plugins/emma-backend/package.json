{
  "name": "@emma-community/backstage-plugin-emma-backend",
  "description": "Backend for the emma plugin",
  "version": "1.0.0",
  "backstage": {
    "role": "backend-plugin",
    "pluginId": "emma",
    "pluginPackages": [
      "@emma-community/backstage-plugin-emma",
      "@emma-community/backstage-plugin-emma-backend",
      "@emma-community/backstage-plugin-emma-common"
    ]
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "private": false,
  "homepage": "https://emma.ms",
  "repository": {
    "type": "git",
    "url": "https://github.com/emma-community/emma-backstage-plugins",
    "directory": "plugins/emma-backstage-plugins"
  },
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist",
    "config.d.ts",
    "src/api/knownGeoLocations.json"
